{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "ext-zip": "*",
        "awobaz/compoships": "^2.1",
        "barryvdh/laravel-dompdf": "^2.0",
        "bavix/laravel-wallet": "*",
        "cybercog/laravel-ban": "^4.9",
        "doctrine/dbal": "^3.3",
        "fruitcake/laravel-cors": "^2.0.5",
        "google/apiclient": "^2.14",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^2.14",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.4",
        "laravelcollective/html": "^6.3",
        "maatwebsite/excel": "^3.1",
        "mcamara/laravel-localization": "^2.0",
        "psr/simple-cache": "^2.0",
        "razorpay/razorpay": "2.*",
        "realrashid/sweet-alert": "^7.2",
        "sentry/sentry-laravel": "^4.8",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-tags": "^4.6",
        "stancl/tenancy": "^3.8",
        "staudenmeir/belongs-to-through": "^2.5",
        "staudenmeir/eloquent-has-many-deep": "^1.7",
        "stripe/stripe-php": "^10.0",
        "unicodeveloper/laravel-paystack": "^1.1",
        "wikimedia/composer-merge-plugin": "^2.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/settings_helper.php",
            "app/Helpers/notification_helper.php",
            "app/Helpers/DeviceHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "pre-autoload-dump": "Google\\Task\\Composer::cleanup"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "merge-plugin": {
            "include": [
                "packages/*/composer.json"
            ]
        },
        "google/apiclient-services": [
            "FirebaseCloudMessaging"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
